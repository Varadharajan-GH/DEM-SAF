				<xsd:enumeration value="Press Digest"/>
				<xsd:enumeration value="Proceedings Paper"/>
				<xsd:enumeration value="Record Review"/>
				<xsd:enumeration value="Reprint"/>
				<xsd:enumeration value="Review"/>
